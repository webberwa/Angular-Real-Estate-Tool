type Investment {
  id: ID! @unique
  address: String!
  price: Float
  monthly_rent: Float
  mortgage_downpayment: Float
  mortgage_amount: Float
  mortgage_interest_rate: Float
  mortgage_period: Float
}

type Provider {
  id: ID! @unique
  name: String!
  type: String!
  rate: Float
  review_number: Int
  review: String
  phone_number: String
  email: String
  addr1: String
  addr2: String
}

type User {
  id: ID! @unique
  email: String! @unique
  password: String!
}
