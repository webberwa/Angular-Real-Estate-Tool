type Investment {
  id: ID! @unique
  address: String!
  price: Float
  monthly_rent: Float
  mortage_downpayment: Float
  mortage_amount: Float
  mortage_interest_rate: Float
  mortage_period: Float
}

type Provider {
  id: ID! @unique
  name: String!
  phone: Int
}

type User {
  id: ID! @unique
  email: String! @unique
  password: String!
}
